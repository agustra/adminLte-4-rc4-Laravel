{
    "env": {
        "browser": true,
        "es2021": true,
        "jquery": true
    },
    "extends": ["eslint:recommended"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "indent": ["error", 4],
        "quotes": ["error", "double"],
        "semi": ["error", "always"],
        "no-unused-vars": "warn",
        "no-console": "off"
    },
    "globals": {
        "bootstrap": "readonly",
        "createEasyTable": "readonly",
        "createEasyTableBulkDelete": "readonly"
    }
}